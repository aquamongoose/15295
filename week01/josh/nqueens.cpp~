#include <bits/stdc++.h>

using namespace std;

int w, h, n;
int moo[100][100];

bool check() {
  for (int i = 0; i < w; i++) {
    for (int j = 0; j < h; j++) {
      for (int k = i + 1; k < w; k++) {
        for (int l = j + 1; l < h; l++) {
          if (moo[i][j] == 0 || moo[k][l] == 0) continue;
          if (i == k) return false;
          if (j == l) return false;
          if (k - i == l - j) return false;
          if (k - i == j - l) return false;
        }
      }
    }
  }
  return true;
}

int dfs (int at, int left) {
  if (left == 0) return 1;
  if (at >= h) return 0;
  int tot = 0;
  for (int i = 0; i < w; i++) {
    moo[i][at] = 1;
    if (check()) tot+= dfs (at + 1, left - 1);
    moo[i][at] = 0;
  }
  tot += dfs(at + 1, left);
  return tot;
}

int main() {
  scanf ("%d %d %d", &w, &h, &n);
  printf ("%d\n", dfs(0, n));
}
